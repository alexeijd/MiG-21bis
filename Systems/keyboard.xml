<PropertyList>
	<key n="4">
		<name>Ctrl-d</name>
		<desc>Deselect All Stations</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station1/selected</property>
			<value type="int">0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station2/selected</property>
			<value type="int">0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station3/selected</property>
			<value type="int">0</value>
		</binding>
	</key>

	<key n="33">
		<name>Shift 1</name>
		<desc>Add Station 2</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station2/selected</property>
			<value type="int">1</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station4/selected</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="35">
		<name>Shift 3</name>
		<desc>Add Station 3</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station3/selected</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="49">
		<name>1</name>
		<desc>Add Station 1</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station1/selected</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="50">
		<name>2</name>
		<desc>Add Station 5</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/station5/selected</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="87">
		<name>Shift-W</name>
		<desc>Fuel cut off</desc>
		<binding>
			<condition>
				<property>/sim/input/selected/engine[0]</property>
			</condition>
			<command>property-toggle</command>
			<property>/controls/engines/engine[0]/cutoff</property>
		</binding>
	</key>

	<key n="99">
		<name>c</name>
		<desc>toggle canopy</desc>
		<binding>
			<command>property-toggle</command>
			<property>fdm/jsbsim/systems/canopy/command</property>
		</binding>
	</key>

	<key n="100">
		<name>d</name>
		<desc>Deploy braking parachute</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/chute/deploy</property>
			<value type="int">1</value>
		</binding>
	</key>

	<!-- needs to be rebound <key n="101">
		<name>e</name>
		<desc>Eject</desc>
		<binding>
			<command>nasal</command>
			<script>Eject.eject();</script>
		</binding>
	</key> -->

	<key n="102">
		<name>f</name>
		<desc>Gun trigger</desc>
		<binding>
			<command>property-assign</command>
			<property>controls/armament/trigger</property>
			<value>true</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>controls/armament/trigger</property>
				<value>false</value>
			</binding>
		</mod-up>
	</key>

	<key n="106">
		<name>j</name>
		<desc>jettison braking parachute</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/chute/deploy</property>
			<value type="int">0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>controls/flight/chute_jettisoned</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="109">
		<name>m</name>
		<desc>engage nose wheel steering</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/NWS/engaged</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/NWS/engaged</property>
				<value>0</value>
			</binding>
		</mod-up>
	</key>

	<key n="113">
		<name>q</name>
		<desc>Release selected store</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/release</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/armament/release</property>
				<value>0</value>
			</binding>
		</mod-up>
	</key>

	<key n="81">
		<name>Shift-Q</name>
		<desc>Center Pylon Drop</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/heavy-release</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/armament/heavy-release</property>
				<value>0</value>
			</binding>
		</mod-up>
	</key>

	<key n="115">
		<name>s</name>
		<desc>Toggle starter valves</desc>
		<binding>
			<command>property-toggle</command>
			<property>controls/engines/engine[0]/starter</property>
		</binding>
		<mod-up>
			<binding>
				<command>null</command>
			</binding>
		</mod-up>
	</key>

	<key n="6">
		<name>Ctrl-f</name>
		<desc>Toggle CSR Mode (modelled as 3 minutes water injection)</desc>
		<binding>
			<command>property-toggle</command>
			<property>fdm/jsbsim/propulsion/clr</property>
		</binding>
	</key>
	
	<key n="110">
		<name>n</name>
		<desc>Lock on target</desc>
		<binding>
			<command>nasal</command>
			<script>radar_logic.lockTarget();</script>
		</binding>
	</key>
	
	<key n="78">
		<name>N</name>
		<desc>Unlock Target</desc>
		<binding>
			<command>nasal</command>
			<script>radar_logic.unlockTarget();</script>
		</binding>
	</key>

	<key n="105">
		<name>i</name>
		<desc>Move locking box up</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/radar/lock-bars-pos</property>
			<step>10</step>
			<min>0</min>
			<max>900</max>
			<wrap>false</wrap>
		</binding>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/pipper-scale</property>
			<step>4</step>
			<min>5</min>
			<max>220</max>
			<wrap>false</wrap>
		</binding>
	</key>
	
	<key n="73">
		<name>I</name>
		<desc>Move locking box up - fine</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/radar/lock-bars-pos</property>
			<step>2</step>
			<min>0</min>
			<max>900</max>
			<wrap>false</wrap>
		</binding>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/pipper-scale</property>
			<step>1</step>
			<min>5</min>
			<max>220</max>
			<wrap>false</wrap>
		</binding>
	</key>
	
	<key n="107">
		<name>k</name>
		<desc>Move locking box down</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/radar/lock-bars-pos</property>
			<step>-10</step>
			<min>0</min>
			<max>900</max>
			<wrap>false</wrap>
		</binding>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/pipper-scale</property>
			<step>-4</step>
			<min>5</min>
			<max>220</max>
			<wrap>false</wrap>
		</binding>
	</key>
	
	<key n="75">
		<name>K</name>
		<desc>Move locking box down - fine</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/radar/lock-bars-pos</property>
			<step>-2</step>
			<min>0</min>
			<max>900</max>
			<wrap>false</wrap>
		</binding>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/pipper-scale</property>
			<step>-1</step>
			<min>5</min>
			<max>220</max>
			<wrap>false</wrap>
		</binding>
	</key>
	
	<key n="83">
		<name>S</name>
		<desc>Activate Smokepod</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-toggle</command>
			<property>/controls/smokepod/trigger</property>
		</binding>
	</key>
</PropertyList>